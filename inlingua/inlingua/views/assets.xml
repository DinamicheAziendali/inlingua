<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Timetable component assets -->
    <template id="assets" name="timetable assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link type="text/css" rel="stylesheet" href="/inlingua/static/src/css/ttmain.css" charset="utf-8"/>
            <link type="text/css" rel="stylesheet" href="/inlingua/static/src/css/scheduler.material.css" charset="utf-8"/>

            <!-- timeline -->
            <script src="/inlingua/static/src/js/libs/custom.locale.it.js"></script>
            <script src='/inlingua/static/src/js/libs/luxon.js' type="text/javascript" charset="utf-8"/>
            <script data-default-locale="It" src='/inlingua/static/src/js/libs/scheduler.umd.js' type="text/javascript" charset="utf-8"></script>

            <!-- core component -->
            <script type="text/javascript" src="/inlingua/static/src/js/timetable_core.dist.js"></script>
        </xpath>
    </template>
</odoo>
